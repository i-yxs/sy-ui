		"navigationBarBackgroundColor": "#437bea",
		"navigationBarTextStyle":"white"
	},
	"easycom": {
		"autoscan": true,
		"custom": {
		  "^sy-(.*)": "@/components/sy-ui/components/sy-$1/sy-$1.vue"
		}
